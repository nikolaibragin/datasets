    "import json\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "from sklearn.neighbors import NearestNeighbors\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "\n",
    "N_NEIGHBORS = 10\n",
    "\n",
    "TDE_TARGET_OBJECT_IDS = ['ZTF22abegjtx', 'ZTF22abkfhua', 'ZTF23aapyidj', 'ZTF23abohtqf', \n",
    "                         'ZTF24aaecooj', 'ZTF24aajvvhj', 'ZTF24aakaiha', 'ZTF24aamfius', \n",
    "                         'ZTF24aatxshz', 'ZTF24abfaake']\n",
    "\n",
    "SLNS_TARGET_OBJECT_IDS = ['ZTF25aanxtou', 'ZTF25aaixrfr', 'ZTF24abvftmi', 'ZTF24abdhylt', \n",
    "                          'ZTF24aaysowl', 'ZTF23aboebgh', 'ZTF23abcvbqq', 'ZTF23aanptpp', \n",
    "                          'ZTF22abcvfgs', 'ZTF21ackxdos']\n",
    "\n",
    "CV_TARGET_OBJECT_IDS = ['ZTF18abjcuhv', 'ZTF18aaudzmj', 'ZTF19aagxcga', 'ZTF18aaashju', \n",
    "                        'ZTF18abigrzf', 'ZTF18adibadz']\n",
    "\n",
    "FEATURE_COLUMNS = [\n",
    "    'ra', 'dec', 'distnr', 'magnr',\n",
    "    'mean_g', 'weighted_mean_g', 'standard_deviation_g', 'median_g', \n",
    "    'amplitude_g', 'beyond_1_std_g', 'cusum_g', 'inter_percentile_range_10_g', \n",
    "    'kurtosis_g', 'linear_trend_g', 'linear_trend_sigma_g', 'linear_trend_noise_g', \n",
    "    'linear_fit_slope_g', 'linear_fit_slope_sigma_g', 'linear_fit_reduced_chi2_g', \n",
    "    'magnitude_percentage_ratio_40_5_g', 'magnitude_percentage_ratio_20_10_g', \n",
    "    'maximum_slope_g', 'median_absolute_deviation_g', 'median_buffer_range_percentage_10_g', \n",
    "    'percent_amplitude_g', 'mean_variance_g', 'anderson_darling_normal_g', \n",
    "    'chi2_g', 'skew_g', 'stetson_K_g',\n",
    "    'mean_r', 'weighted_mean_r', 'standard_deviation_r', 'median_r', \n",
    "    'amplitude_r', 'beyond_1_std_r', 'cusum_r', 'inter_percentile_range_10_r', \n",
    "    'kurtosis_r', 'linear_trend_r', 'linear_trend_sigma_r', 'linear_trend_noise_r', \n",
    "    'linear_fit_slope_r', 'linear_fit_slope_sigma_r', 'linear_fit_reduced_chi2_r', \n",
    "    'magnitude_percentage_ratio_40_5_r', 'magnitude_percentage_ratio_20_10_r', \n",
    "    'maximum_slope_r', 'median_absolute_deviation_r', 'median_buffer_range_percentage_10_r', \n",
    "    'percent_amplitude_r', 'mean_variance_r', 'anderson_darling_normal_r', \n",
    "    'chi2_r', 'skew_r', 'stetson_K_r'\n",
    "]\n",
    "\n",
    "print(\"Loading data...\")\n",
    "df = pd.read_parquet(PARQUET_FILE)\n",
    "print(f\"Total objects: {len(df)}\")\n",
    "\n",
    "df_features = df[['objectId'] + FEATURE_COLUMNS].copy()\n",
    "df_features.replace([np.inf, -np.inf], np.nan, inplace=True)\n",
    "df_features = df_features.dropna()\n",
    "\n",
    "X = df_features[FEATURE_COLUMNS].values\n",
    "object_ids = df_features['objectId'].values\n",
    "id_to_idx = {obj_id: idx for idx, obj_id in enumerate(object_ids)}\n",
    "\n",
    "print(f\"Objects after dropping NaN: {len(df_features)}\")\n",
    "print(f\"Feature matrix shape: {X.shape}\")\n",
    "\n",
    "print(\"Scaling features...\")\n",
    "scaler = StandardScaler()\n",
    "X_scaled = scaler.fit_transform(X)\n",
    "\n",
    "print(\"Building KNN model...\")\n",
    "knn = NearestNeighbors(n_neighbors=N_NEIGHBORS + 1, metric='euclidean', n_jobs=-1)\n",
    "knn.fit(X_scaled)\n",
    "\n",
    "\n",
    "def find_neighbors(target_ids, group_name):\n",
    "    print(f\"\\n{'='*80}\")\n",
    "    print(f\"{group_name} NEIGHBORS\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    results = {}\n",
    "    \n",
    "    for target_id in target_ids:\n",
    "        print(f\"\\n{target_id}:\")\n",
    "        \n",
    "        if target_id not in id_to_idx:\n",
    "            print(f\"  NOT FOUND in data!\")\n",
    "            continue\n",
    "        \n",
    "        target_idx = id_to_idx[target_id]\n",
    "        target_vector = X_scaled[target_idx].reshape(1, -1)\n",
    "        distances, indices = knn.kneighbors(target_vector)\n",
    "        \n",
    "        neighbors = []\n",
    "        rank = 0\n",
    "        for dist, idx in zip(distances[0], indices[0]):\n",
    "            neighbor_id = object_ids[idx]\n",
    "            if neighbor_id == target_id:\n",
    "                continue\n",
    "            rank += 1\n",
    "            neighbors.append(neighbor_id)\n",
    "            print(f\"  {rank:2}. {neighbor_id} (distance: {dist:.4f})\")\n",
    "        \n",
    "        results[target_id] = neighbors\n",
    "    \n",
    "    return results\n",
    "\n",
    "\n",
    "tde_results = find_neighbors(TDE_TARGET_OBJECT_IDS, 'TDE')\n",
    "slns_results = find_neighbors(SLNS_TARGET_OBJECT_IDS, 'SLNS')\n",
    "cv_results = find_neighbors(CV_TARGET_OBJECT_IDS, 'CV')\n",
    "\n",
    "with open('tde_neighbors.json', 'w') as f:\n",
    "    json.dump(tde_results, f, indent=2)\n",
    "print(f\"\\nSaved: tde_neighbors.json\")\n",
    "\n",
    "with open('slns_neighbors.json', 'w') as f:\n",
    "    json.dump(slns_results, f, indent=2)\n",
    "print(f\"Saved: slns_neighbors.json\")\n",
    "\n",
    "with open('cv_neighbors.json', 'w') as f:\n",
    "    json.dump(cv_results, f, indent=2)\n",
    "print(f\"Saved: cv_neighbors.json\")\n",
    "\n",
    "all_results = {\n",
    "    'TDE': tde_results,\n",
    "    'SLNS': slns_results,\n",
    "    'CV': cv_results\n",
    "}\n",
    "\n",
    "with open('all_neighbors.json', 'w') as f:\n",
    "    json.dump(all_results, f, indent=2)\n",
    "print(f\"Saved: all_neighbors.json\")\n",
    "\n",
    "print(\"\\n\" + \"=\"*80)\n",
